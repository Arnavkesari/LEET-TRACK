# MongoDB Configuration
MONGODB_URI=mongodb://localhost:27017/leetcode-tracker
DB_NAME=leetcode-tracker

# JWT Configuration
ACCESS_TOKEN_SECRET=your-super-secret-access-token-key-here-change-in-production
ACCESS_TOKEN_EXPIRY=1d
REFRESH_TOKEN_SECRET=your-super-secret-refresh-token-key-here-change-in-production
REFRESH_TOKEN_EXPIRY=10d

# Server Configuration
PORT=8000
NODE_ENV=development

# CORS Configuration
CORS_ORIGIN=http://localhost:5173

# Rate Limiting (requests per window)
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# LeetCode Scraping Configuration
SCRAPING_DELAY_MS=2000
MAX_CONCURRENT_SCRAPES=3
